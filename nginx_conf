server {
    listen 80;
    server_name <server-name>;

    location / {
        uwsgi_pass unix:/tmp/uwsgi.sock;
        include uwsgi_params;
        uwsgi_buffering on;
        uwsgi_buffer_size 64k;
        uwsgi_buffers 4 64k;
        uwsgi_busy_buffers_size 64k;
        uwsgi_read_timeout 300s;
    }
}
